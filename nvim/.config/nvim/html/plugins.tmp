<span class="n">Plug</span> <span class="s">&#39;junegunn/fzf&#39;</span><span class="p">,</span> <span class="p">{</span> <span class="s">&#39;dir&#39;</span><span class="o">:</span> <span class="s">&#39;~/.fzf&#39;</span><span class="p">,</span> <span class="s">&#39;do&#39;</span><span class="o">:</span> <span class="s">&#39;./install --all&#39;</span> <span class="p">}</span>
<span class="n">Plug</span> <span class="s">&#39;junegunn/fzf.vim&#39;</span>

