Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
!Xft.dpi: 96

!! URxvt
URxvt*font: xft:Fira Code:pixelsize=14:antialias=true:hinting=true
URxvt*boldFont: xft:Fira Code:bold:pixelsize=14:antialias=true:hinting=true
xterm*faceName: xft:FiraCode:size=13:antialias=true
URxvt*letterSpace: -1

